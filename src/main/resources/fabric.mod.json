		"main": [
			{
				"value": "com.edelweiss.SkillAttributes",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.edelweiss.SkillAttributesClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"skill-attributes.mixins.json",
